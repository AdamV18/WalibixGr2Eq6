<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
    <top>
        <ImageView fx:id="logo" fitHeight="100.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="TOP_LEFT">
            <image>
                <Image url="@../../../images/logo_walibix.png" />
            </image>
        </ImageView>
    </top>
    <center>
        <VBox alignment="CENTER" spacing="30">
            <children>
                <Label fx:id="texteFinReservation" alignment="CENTER" text="Réservation finie !">
                    <font>
                        <Font size="40.0" />
                    </font>
                </Label>
                <Button fx:id="boutonRetourAccueil" mnemonicParsing="false" text="Retourner à l'accueil" />
            </children>
        </VBox>
    </center>
</BorderPane>
